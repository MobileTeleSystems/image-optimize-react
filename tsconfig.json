{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "allowSyntheticDefaultImports": true,
        "declaration": false,
        "diagnostics": false,
        "downlevelIteration": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "jsx": "react-jsx",
        "lib": [
            "ES2020",
            "dom",
            "dom.iterable"
        ],
        "importHelpers": true,
        "noEmitHelpers": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "preserveConstEnums": true,
        "pretty": true,
        "strict": true,
        "strictFunctionTypes": false,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "outDir": "./dist/esnext",
        "noEmitOnError": true,
        "baseUrl": ".",
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [

    ],
    "ts-node": {

    }
}